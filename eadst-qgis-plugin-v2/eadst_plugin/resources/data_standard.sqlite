INSERT INTO breeds (breed_code, name, species_code, breed_type_code, abbreviation, coat_color, horns, location, pictorial_description_available, description) VALUES
('2.01.01', 'Abergele', 2, 1, 'ABEg', 'Mostly reddish brown; plain coat color, patchy, and rarely spotted', 'Males have spiral horns directed backwards', 'South Tigray and North Wollo, Ethiopia/Eritrea', 1, 'Wattles are almost entirely absent'),
('2.01.02', 'Afar', 2, 1, 'AFAg', 'Coat is very fine and short with variable colors (white, light brown, black, black, and flecks), and patches are also common', 'Long, thin, upward-pointing horns', 'Afar, northern and western Hararghe', 1, 'A beard (common in males) and wattle are relatively common'),
('2.01.03', 'Agew', 2, 1, 'AGE', 'White and/or fawn color', NULL, 'Highlands of South Gonder, Gojam, Wellega and western Shoa; also found in Gojam, Wolega, Ilu Aba Bora', 1, NULL),
('2.01.04', 'Arab', 2, 1, 'ARA', 'Patchy pattern or smooth white or black', 'Straight, curved, or spiral with upward or backward orientation', 'Benishangul', 1, 'Wattle and beard present'),
('2.01.05', 'Arsi-Bale', 2, 1, 'ARS-BAL', 'White (male) or brown (female), also black-and-gray, roan, red', 'Males have curved or straight horns mainly pointing backwards, but in some, pointed straight upwards; there are few polled goats in both sexes', 'Highlands of Arsi, Bale and South Shewa, Ethiopia', 1, NULL),
('2.01.06', 'Begayit', 2, 1, 'BEGg', 'Mainly white with brown patches; their hair is particularly long around the thighs', 'Both sexes have horns that are straight or curved and oriented backwards; males have beards and ruffs', 'Found mainly in western Tigray', 1, NULL),
('2.01.07', 'Central Highland', 2, 1, 'CEN-HIG', 'Predominant color is reddish-brown, followed by plain, patchy, and rarely spotted', 'Males have horns that are thick, mostly straight and pointed backwards; horns are sometimes curved and rarely spiral', 'Highlands of North Ethiopia and South Eritrea', 1, 'Wattles: Rarely present; commonly have beard and ruff'),
('2.01.08', 'Felata', 2, 1, 'FEL', 'Most are patchy pattern', 'Straight or curved backward or upward', 'Northwestern lowlands (Bambasi and Mandura)', 1, NULL),
('2.01.09', 'Gumuz', 2, 1, 'GUM', 'The dominant color is white followed by fawn, black, and gray; white patchy colors are also commonly observed', 'Both males and females have straight and backward-oriented horns; most males also have beards and ruffs', 'Found in areas bordering the Sudan (Metekel, Assosa, and Gambela)', 1, NULL),
('2.01.10', 'Hararghe Highland', 2, 1, 'HAR-HIG', 'The coat color is white, brown, or black and the hair is short and shiny', 'Commonly polled; horned goats have straight or curved horns', 'Found in the East and West Hararghe highlands', 1, NULL),
('2.01.11', 'Keffa', 2, 1, 'KEF', 'Coarse, hairy coat, dominant colors are black or brown', 'Most males have straight and backward-oriented horns', 'Widely distributed in the highlands and lowlands of Keffa', 1, 'Wattles: Present in some; males have beards and ruffs'),
('2.01.12', 'Ille', 2, 1, 'ILLg', 'no information on its phenotypic description can be found', NULL, NULL, 0, NULL),
('2.01.13', 'Long-eared Somali', 2, 1, 'LON-EAR-SOM', 'Short, smooth coat that is mainly white, with occasional brown, black, and gray', 'Mainly curved or pointed backwards; some of the horns in both sexes have a lateral orientation; high incidence of polledness', 'Found in all parts of the Ogaden, lowlands of Bale and Borana zones of Oromia, and some parts of Sidama Region', 1, NULL),
('2.01.14', 'Maefur', 2, 1, 'MAE', 'Spotty, pied, and uniform and shaded (light brown, black, and gray).', 'Long and thick', 'Mountainous topographical features of the hillside of Erob district eastern Tigray', 1, NULL),
('2.01.15', 'Short-eared Somali', 2, 1, 'SHO-EAR_SOM', 'Mainly white; short, smooth hair', 'Upward pointing horns; very few polled with a low incidence of spiral horns in both sexes', 'Northern and eastern Ogaden, Dire Dawa', 1, NULL),
('2.01.16', 'Western Highland', 2, 1, 'WES-HIG', 'Mostly covered with coarse hair, forming a long coat; most observed color is plain followed by patchy and spotted color patterns', 'Both males and females have horns but some animals are polled; mostly straight and backward-oriented', 'Highlands of South Gonder, Gojam, Wellega and Western Shoa', 1, 'Wattles: Present in some; males also have beards and ruffs'),
('2.01.17', 'Western Lowlands', 2, 1, 'WES-LOW', 'Fawn (often with white patches), occasionally black or gray', 'Most male goats have straight horns, oriented backwards; few polled males in the population', 'Western lowlands bordering Sudan around Metekel, Assosa, and Ilu Aba Bora', 1, NULL),
('2.01.18', 'Widar', 2, 1, 'WID', 'Coat is very fine and short with variable colors and color patterns—white, light, patchy brown, black, and flecks', 'Long, thin, upward-pointing horns', 'Afar, northern and western Hararghe, Eritrean rift valley strip', 1, NULL),
('2.01.19', 'Woyito-Guji', 2, 1, 'WOY-GUJ', 'The body is covered with shiny and smooth hair of various colors; predominant coat colors are reddish-brown in a patchy pattern with black or brown stripes running along the back, on the underside, or on the front of the legs', 'Most males and females are horned; some animals are polled; horns are mostly oriented backward or upward, sometimes laterally', 'North and South Omo, southern Sidama and parts of Wolayta', 1, 'Wattles: Present in some; males have beards and ruffs');

