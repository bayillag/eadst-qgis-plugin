-- Table from Urban, peri-urban or rural livestock production system field (Page 125)
CREATE TABLE production_system_enum (
    system TEXT PRIMARY KEY
);

-- Table from Intensive, Extensive, Semi-intensive livestock production system field (Page 126)
CREATE TABLE investment_level_enum (
    level TEXT PRIMARY KEY
);

-- Table from Table 71: Herd/Flock Size (Extensive System) (Page 126)
CREATE TABLE farm_size_extensive (
    species TEXT NOT NULL,
    system_type TEXT NOT NULL, -- Pastoral, Agro-Pastoral, Mixed Farming
    small_range TEXT,
    medium_range TEXT,
    large_range TEXT,
    PRIMARY KEY (species, system_type)
);

-- Table from Table 72: Feedlot and Dairy Size Based (Intensive, Semi-Intensive) (Page 127)
CREATE TABLE farm_size_intensive (
    species TEXT NOT NULL,
    system_subtype TEXT NOT NULL, -- e.g., 'Feedlot Semi-intensive'
    small_range TEXT,
    medium_range TEXT,
    large_range TEXT,
    PRIMARY KEY (species, system_subtype)
);

-- Table from Farm Size field (Page 127)
CREATE TABLE farm_size_enum (
    size TEXT PRIMARY KEY
);

-- Table from Table 74: Cattle Age Determination Based on Dentition (Page 128)
CREATE TABLE cattle_age_dentition (
    age_of_cattle TEXT PRIMARY KEY,
    description_of_teeth TEXT
);

-- Table from Table 75: Camel Age Determination Based on Dentition (Page 129)
CREATE TABLE camel_age_dentition (
    age TEXT PRIMARY KEY,
    features TEXT
);

-- Table from Table 76: Age Estimated for Sheep and Goats Based on Dentition (Page 130)
CREATE TABLE ruminant_age_dentition (
    no_of_permanent_incisors TEXT PRIMARY KEY,
    goat_age_years TEXT,
    sheep_age_years TEXT
);

-- Table from Table 77: Categories of Cattle BCS (1 to 5) (Page 131)
CREATE TABLE cattle_bcs_feedlot (
    score INTEGER PRIMARY KEY,
    body_condition TEXT,
    fatness TEXT,
    description TEXT
);

-- Table from Table 78: Cattle Body Conformation Score (Page 132)
CREATE TABLE cattle_conformation_score_feedlot (
    score INTEGER PRIMARY KEY,
    grading TEXT,
    description TEXT
);

-- Table from Table 79: Cattle Grading Based on the Five Parameters (Page 133)
CREATE TABLE cattle_grading_feedlot (
    grade INTEGER,
    sex TEXT,
    weight_kg TEXT,
    age_years TEXT,
    bcs TEXT,
    bcofs TEXT,
    PRIMARY KEY (grade, sex, age_years)
);

-- Table from Table 80: Lactating Cattle BCS (Page 134)
CREATE TABLE cattle_bcs_dairy (
    score INTEGER PRIMARY KEY,
    condition TEXT,
    description TEXT
);

-- Table from Table 81: Recommended BCS for different lactation stages of dairy cattle (Page 135)
CREATE TABLE cattle_bcs_dairy_targets (
    lactation_stage TEXT PRIMARY KEY,
    recommended_target_bcs TEXT
);

-- Table from Table 82: Percentage score of body conformation with description (Page 136)
CREATE TABLE dairy_conformation_traits (
    observed_structure TEXT PRIMARY KEY,
    scored_value_percent INTEGER,
    description TEXT
);

-- Table from Table 83: Camel body condition score (Page 138)
CREATE TABLE camel_bcs (
    score INTEGER PRIMARY KEY,
    body_condition TEXT,
    fatness TEXT,
    description TEXT
);

-- Table from Table 84: Camel body conformation with its description (Page 139)
CREATE TABLE camel_conformation (
    score INTEGER PRIMARY KEY,
    body_conformation TEXT,
    description TEXT
);

-- Table from Table 85: Camel grading in relation to age, sex, BCS, and conformation (Page 140)
CREATE TABLE camel_grading (
    grade TEXT,
    age_years TEXT,
    sex TEXT,
    body_condition TEXT,
    body_conformation TEXT,
    PRIMARY KEY (grade, age_years, sex)
);

-- Table from Table 86: Scales for BSC of Sheep and Goats (Page 141)
CREATE TABLE ruminant_bcs (
    score INTEGER PRIMARY KEY,
    body_condition TEXT,
    lumbar_region_desc TEXT,
    rib_cage_desc TEXT,
    sternum_desc TEXT
);

-- Table from Table 87: Sheep and Goat Grading Based on Body Conformation (Page 143)
CREATE TABLE ruminant_conformation (
    score INTEGER PRIMARY KEY,
    conformation TEXT,
    description TEXT
);

-- Table from Table 88: Grades and Grading Parameters for Male Sheep and Goats (Page 144)
CREATE TABLE ruminant_grading_male (
    grade INTEGER PRIMARY KEY,
    age_months_sheep TEXT,
    age_months_goat TEXT,
    live_body_weight_kg_sheep TEXT,
    live_body_weight_kg_goat TEXT,
    body_condition_score TEXT,
    body_conformation_score TEXT
);

