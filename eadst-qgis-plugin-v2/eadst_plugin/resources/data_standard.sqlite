-- #############################################
-- SECTION: GENERAL INFORMATION & ENUMS
-- #############################################

-- Table from Acronyms section (Page vii-viii)
CREATE TABLE acronyms (
    acronym TEXT PRIMARY KEY,
    full_name TEXT NOT NULL
);

-- Table from Age Categorization (Page xvi)
CREATE TABLE age_categories (
    species TEXT PRIMARY KEY,
    immature_calf_years TEXT,
    young_years TEXT,
    adult_years TEXT,
    old_years TEXT
);

-- Table from Table 1: Country Code and Range of Code Numbers by Species (Page 23)
CREATE TABLE animal_id_ranges (
    species TEXT PRIMARY KEY,
    country_code TEXT,
    start_range INTEGER,
    end_range INTEGER,
    total_tagged INTEGER,
    display_format_start TEXT,
    display_format_end TEXT
);

-- Table from Table 4: Scientific Names of Different Domestic Animal Species (Page 25)
CREATE TABLE species (
    id INTEGER PRIMARY KEY,
    common_name TEXT UNIQUE NOT NULL,
    scientific_name TEXT,
    subfamily TEXT
);

-- Table from Table 9: Codes of Various Animal Species (Page 29)
CREATE TABLE species_codes (
    species_name TEXT PRIMARY KEY,
    species_code INTEGER UNIQUE NOT NULL
);

-- Table from Table 9: Codes of Breed Types (Page 29)
CREATE TABLE breed_type_codes (
    breed_type TEXT PRIMARY KEY,
    breed_type_code INTEGER UNIQUE NOT NULL
);

-- Table from Table 8: Possible Blood Level Percentage of Crosses Between Exotic and Indigenous Animals (Page 29)
CREATE TABLE crossbreeding_filial_levels (
    crossbreeding TEXT PRIMARY KEY,
    filial TEXT,
    exotic_blood_level_percent REAL,
    exotic_blood_level_fraction TEXT
);

-- Table from Table 24: Production Purposes of Domestic Animals in Ethiopia (Page 37)
CREATE TABLE production_purposes (
    purpose TEXT PRIMARY KEY,
    species_affected TEXT
);

-- Table from Table 26: Reproduction Status Categories (Page 37)
CREATE TABLE reproduction_status_enum (
    status TEXT PRIMARY KEY,
    description TEXT
);

-- Table from Table 28, 29, 30: Lactation Status Categories (Page 38-39)
CREATE TABLE lactation_status_enum (
    species TEXT,
    status TEXT,
    description TEXT,
    PRIMARY KEY (species, status)
);

-- Table from Table 34: Animal Health Status Categories (Page 41)
CREATE TABLE health_status_enum (
    status TEXT PRIMARY KEY,
    description TEXT
);

-- #############################################
-- SECTION: BREEDS (Cattle, Goats, Sheep, Camels, etc.)
-- #############################################

-- Master table for all breeds from pages 31-51 and Appendix 1 (Page 167)
CREATE TABLE breeds (
    breed_code TEXT PRIMARY KEY,
    name TEXT NOT NULL,
    species_code INTEGER,
    breed_type_code INTEGER,
    abbreviation TEXT,
    description TEXT,
    synonym TEXT,
    origin TEXT,
    coat_color TEXT,
    horns TEXT,
    hump TEXT,
    location TEXT,
    tail TEXT,
    pictorial_description_available INTEGER, -- Boolean (0 or 1)
    FOREIGN KEY (species_code) REFERENCES species_codes(species_code),
    FOREIGN KEY (breed_type_code) REFERENCES breed_type_codes(breed_type_code)
);

-- Table for crossbreeds from tables 13, 16, 19
CREATE TABLE crossbreeds (
    crossbreed_code TEXT PRIMARY KEY,
    name TEXT,
    abbreviation TEXT,
    description TEXT,
    species_code INTEGER,
    FOREIGN KEY (species_code) REFERENCES species_codes(species_code)
);


