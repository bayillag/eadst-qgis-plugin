-- Table from Tables 59, 60: Disease Categories (Page 102)
CREATE TABLE disease_categories (
    code TEXT PRIMARY KEY,
    name TEXT NOT NULL,
    parent_code TEXT,
    FOREIGN KEY(parent_code) REFERENCES disease_categories(code)
);

-- Table from Tables 61, 62, 63: Disease Lists (Pages 104-121)
CREATE TABLE diseases (
    code TEXT PRIMARY KEY,
    name TEXT,
    abbreviation TEXT,
    category_code TEXT,
    species_affected TEXT,
    causative_agent TEXT,
    other_names TEXT,
    notes TEXT,
    FOREIGN KEY(category_code) REFERENCES disease_categories(code)
);

-- Table from Table 64: Diagnostic Methods and Their Abbreviations (Page 126)
CREATE TABLE diagnostic_methods (
    code TEXT PRIMARY KEY,
    method TEXT,
    abbreviation TEXT
);

-- Table from Table 65: Possible Treatment and Their Abbreviations (Page 129)
CREATE TABLE treatments (
    code TEXT PRIMARY KEY,
    treatment_name TEXT,
    abbreviation TEXT
);

-- Table from Table 66: Husbandry Procedures and Their Abbreviations (Page 129)
CREATE TABLE husbandry_procedures (
    code TEXT PRIMARY KEY,
    procedure_name TEXT,
    abbreviation TEXT
);

-- Table from Table 67 & 68: Vaccines (Pages 132-135)
CREATE TABLE vaccines (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT,
    abbreviation TEXT,
    code TEXT,
