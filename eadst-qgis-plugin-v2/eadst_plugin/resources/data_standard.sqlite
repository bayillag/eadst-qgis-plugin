-- Data for: breeds (Cattle - Indigenous)
INSERT INTO breeds (breed_code, name, species_code, breed_type_code, abbreviation, coat_color, horns, hump, location, synonym, pictorial_description_available) VALUES
('1.01.01', 'Abergele', 1, 1, 'ABE', 'Fine skin, coat color varies considerably with black, spotted, and black and white spotted; black and white animals are abundant color types; other colors include chestnut, gray, and light red', 'Females have thin and medium-sized horns, mostly upward with pointed ends; males have thicker and shorter horns; polled animals are not common', 'Small and not very well developed; small dewlap and naval flap are typical features of the breed', 'Southern Tigray and North Wollo', NULL, 1),
('1.01.02', 'Abigar', 1, 1, 'ABI', 'Red or black pied', 'Vary in length and shape, but in general, are very long and crescent-shaped, project outward and upward, or are oval; in some cases, horns are loose, unattached, and short', 'Small', 'Border area between Ethiopia (Akobo area of Gambella), Sudan, Illubabor, Kefa', 'Neolithic, Nuer', 1),
('1.01.03', 'Adwa', 1, 1, 'ADW', 'Mainly red, chestnut, black, roan, and white', 'Upward curving horns', 'Upward curving horns', 'Found around Adwa, in the central zone of the Tigray Region', NULL, 1),
('1.01.04', 'Afar', 1, 1, 'AFA', 'Usually ash gray, cream, or light chestnut', 'Extremely long—up to 1 meter or more in length—and lyre- or crescent-shaped, sometimes forming a complete circle', 'Small cervico-thoracic hump and small dewlap', 'Afar region, Western Hararghe, Eastern Shewa zones, Tigray and Welo, Ethiopia, and Northeastern Djibouti, South Eritrea', 'Danakil', 1),
('1.01.05', 'Ambo', 1, 1, 'AMB', 'No information on phenotypic descriptions', 'No information on phenotypic descriptions', 'No information on phenotypic descriptions', 'Located in west Shewa around Ambo, Dandi, Addis Alem and Holeta areas', NULL, 1),
('1.01.06', 'Arado', 1, 1, 'ARA', 'Mostly red with shades ranging from light to dark pattern; many animals are red-pied or black-pied, and a few of them are black, brown, gray, or white', 'Vary in size and may be large, crescent-shaped, or even absent', 'Smaller in females, larger in males, and the dewlap is relatively large', 'Northern Ethiopia (northern Shire, Adwa and parts of Agame) and in the highlands of Eritrea', 'Ethiopian Highland Zebu, Akele-Guzai, Asaorta, Bileri (Keren)', 1),
('1.01.07', 'Arsi', 1, 1, 'ARS', 'Red, with a black muzzle, is predominant, although many animals are black, light gray, or white with black spots', 'Small and short', 'Small', 'Highlands of Central Ethiopia especially Arsi, Bale, and Shoa', NULL, 1),
('1.01.08', 'Bale', 1, 1, 'BAL', 'Black, chestnut, white, or roan', 'No info', 'Prominent hump', 'High plateau of Bale zone, in areas adjacent to the habitat of the Jem-Jem', 'Highland Zebu', 0),
('1.01.09', 'Begait', 1, 1, 'BEGc', 'Mostly brown, black and white, or beige', 'Small and stumpy, loose hanging down', 'Mostly no hump (a few with a small hump)', 'Tigray and Gondor', 'Barka', 1),
('1.01.10', 'Boran', 1, 1, 'BOR', 'Usually white or gray fawn, but light brown and black/dark brown shading on the head, neck, shoulder, and hindquarter also occur', 'Short, erect, and thick at the base', 'Straight above and in line with front legs, rounded, well developed', 'Borana zone, Southern Ethiopia, Smoli, and border areas (Northern Kenya, Somalia, and other African countries)', 'Borana, Ethiopian Boran, Somalia Boran, or Awai', 1),
('1.01.11', 'Fogera', 1, 1, 'FOG', 'White, white with black spots, or black and gray.', 'Short, stumpy, pointed', 'Ranges from thoracic to cervical-thoracic', 'Northwest Ethiopia, mainly in Gojam (around Lake Tana) and Gindor', 'Wagera', 1),
('1.01.12', 'Goffa', 1, 1, 'GOF', 'Plain black or reddish brown.', 'Small to medium, wedge-like', 'No info', 'Goffa area, principally around Sawla in South Omo.', 'Konso (Knosso)', 1),
('1.01.13', 'Guraghe', 1, 1, 'GUR', 'Usually red, chestnut, or roan', 'Small and short', 'No info', 'Guraghe and Hadiya areas', 'Ethiopian Highland Zebu', 1),
('1.01.14', 'Hammer', 1, 1, 'HAM', 'White, gray, chestnut, or roan usually', 'Short to medium', 'Prominent', 'Found in Hammer area, in Southern Omo', NULL, 1),
('1.01.15', 'Harar', 1, 1, 'HAR', 'White gray, white, red, and black', 'Short and thick', 'Smaller', 'Highlands of Eastern and Western Hararghe Zone', NULL, 1),
('1.01.16', 'Horro', 1, 1, 'HORc', 'Uniform brown color, slightly lighter around the muzzle and on the flank', 'Medium to large', 'Straight and small to medium', 'Wellega, Ilubabor, Keffa, Shoa', 'Wallega', 1),
('1.01.17', 'Jem-Jem Zebu', 1, 1, 'JEM', 'Black, usually with white face or patches on a mainly black background', 'Short', 'Small to medium', 'Bale, Sidama', 'Black Zebu, Black Highland Cattle', 0),
('1.01.18', 'Jiddu', 1, 1, 'JID', 'Ranges from all shades of white and light fawn to dark mahogany or red; in almost all cases, Jiddu cattle have white muzzles, white eyelashes, white hairs', 'Project form pedestal-like', 'Smaller in females than the male; thoracic or cervico-thoracic hump position; the dewlap is moderately well developed and free from folds and fleshiness', 'Southeastern Ethiopia and Southern Somalia, also Zanzibar, Tanzania', NULL, 1),
('1.01.19', 'Jijjiga', 1, 1, 'JIJ', 'Chestnut, black, white, dark gray, or red', 'Short, mostly polled', 'Erect', 'Ogaden area of the Somali region and border area Eastern Hararghe', 'Ogaden', 1),
('1.01.20', 'Medenes', 1, 1, 'MED', 'Black and white in varying patterns, other colors are rare', 'No information on descriptions of horn and hump', 'No information on descriptions of horn and hump', 'Distributed in the hot to warm semi-arid lowlands of Welqayit', NULL, 1),
('1.01.21', 'Mursi', 1, 1, 'MUR', 'Various combinations (solid rare) of gray, white, black, chestnut, and roan, or pied or striped', 'Mainly large, usually curved inwards', 'Prominent and well-developed hump', 'Mursi and Bodi pastoral communities, South Omo zone', NULL, 1),
('1.01.22', 'Ogaden', 1, 1, 'OGA', 'Uniform plain white, black shade around face', 'In most cases polled, but when present, it is short and thick', 'Thoracic in location, large and prominent in both sexes, but larger in males', 'Ogaden area of Somali Region and bordering East Hararghe', NULL, 1),
('1.01.23', 'Raya-Azebo', 1, 1, 'RAY-A', 'Chestnut, ash gray, and black spots', 'Lyre-shaped, upward orientation', 'Long, erect, small, and found at cervical thoracic position', 'Tigray, Wollo, Afar', 'Raya, Harmo', 1),
('1.01.24', 'Sheko', 1, 1, 'SHE', 'Most often brown multi-colored with black and white, however, brown predominates in a plain, patchy, or spotted pattern', 'Short, many are polled', 'Absent or small', 'Southwest Kefa', 'Mitzan, Goda', 1),
('1.01.25', 'Smada', 1, 1, 'SMA', 'Black, also red, roan, or black-and-white', 'Short to moderate', 'Small to medium', 'North Western Ethiopia', 'Worie, Agew', 1);

